{
  "name": "emana-digital/laravel-assets-helper",
  "description": "Laravel Blade helpers for a easy integration with assets-webpack-plugin.",
  "type": "project",
  "license": "MIT",
  "authors": [
    {
      "name": "Alfredo Peres",
      "email": "alfredo@emana.digital"
    }
  ],
  "require": {
    "php": "^7.3|^8.0",
    "illuminate/support": "^8.12"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "extra": {
    "laravel": {
      "providers": [
        "emanadigital\\LaravelAssetsHelperProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "emanadigital\\": "src/"
    }
  }
}
